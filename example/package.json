{
  "name": "example",
  "version": "0.0.0",
  "description": "Example",
  "main": "dist/app.js",
  "private": true,
  "scripts": {
    "build": "npm-run-all test build:*",
    "build:js": "browserify src/app.js -t babelify -o dist/app.js",
    "build:jade": "jade -o dist src",
    "doc": "esdoc -c esdoc.json",
    "lint": "eslint src test",
    "prebuild": "rm -rf dist && mkdir -p dist",
    "server": "browser-sync start --no-open --server dist --files 'dist/*.html,dist/*.js'",
    "start": "npm-run-all --parallel server watch",
    "test": "npm-run-all lint test:*",
    "test:coverage": "babel-node $(npm bin)/isparta cover --report text --report html --report lcovonly _mocha -- test/**/*spec.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:jade": "jade -o dist -w src",
    "watch:js": "watchify src/app.js -v -d -t babelify -o dist/app.js",
    "watch:test": "mocha --watch test/**/*spec.js"
  },
  "author": "moqada <moqada@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.2",
    "browserify": "^13.1.1",
    "clear-require": "^1.0.1",
    "esdoc": "^0.4.8",
    "eslint": "^3.10.2",
    "eslint-config-moqada": "^18.0.0",
    "eslint-plugin-react": "^6.7.1",
    "isparta": "^4.0.0",
    "jade": "^1.11.0",
    "mocha": "^3.1.2",
    "npm-run-all": "^3.1.1",
    "proxyquire": "^1.7.10",
    "react-addons-test-utils": "^15.4.0",
    "testdom": "^2.0.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "flux": "^3.1.0",
    "flux-simple-action-creator": "file:..",
    "react": "^15.4.0",
    "react-dom": "^15.4.0"
  }
}

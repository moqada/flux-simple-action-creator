{
  "name": "example",
  "version": "0.0.0",
  "description": "Example",
  "main": "dist/app.js",
  "private": true,
  "scripts": {
    "build": "npm-run-all test build:*",
    "build:js": "browserify src/app.js -t babelify -o dist/app.js",
    "build:jade": "jade -o dist src",
    "doc": "esdoc -c esdoc.json",
    "lint": "eslint src test",
    "prebuild": "rm -rf dist && mkdir -p dist",
    "server": "browser-sync start --no-open --server dist --files 'dist/*.html,dist/*.js'",
    "start": "npm-run-all --parallel server watch",
    "test": "npm-run-all lint test:*",
    "test:coverage": "babel-node $(npm bin)/isparta cover --report text --report html --report lcovonly _mocha -- test/**/*spec.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:jade": "jade -o dist -w src",
    "watch:js": "watchify src/app.js -v -d -t babelify -o dist/app.js",
    "watch:test": "mocha --watch test/**/*spec.js"
  },
  "author": "moqada <moqada@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-espower": "^2.1.1",
    "babel-plugin-transform-class-properties": "^6.5.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "clear-require": "^1.0.1",
    "esdoc": "^0.4.4",
    "eslint": "^1.10.3",
    "eslint-config-moqada": "^10.0.0",
    "eslint-plugin-react": "^3.16.1",
    "isparta": "^4.0.0",
    "jade": "^1.11.0",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.5.1",
    "proxyquire": "^1.7.4",
    "react-addons-test-utils": "^0.14.7",
    "testdom": "^2.0.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "flux": "^2.1.1",
    "flux-simple-action-creator": "file:..",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
